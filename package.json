{
  "name": "glacier-scan",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered glacier monitoring and analysis platform",
  "keywords": [
    "glacier",
    "ai",
    "monitoring",
    "climate",
    "analysis",
    "nextjs",
    "fastapi",
    "nestjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/glacier-scan.git"
  },
  "license": "MIT",
  "author": "Glacier Scan Team",
  "main": "index.js",
  "scripts": {
    "build": "cd frontend && npm run build",
    "db-status": "npm run verify-db",
    "db:migrate": "psql $DATABASE_URL -f supabase/migrations/001_initial_schema.sql && psql $DATABASE_URL -f supabase/migrations/002_seed_data.sql",
    "dev": "cd frontend && npm run dev",
    "dev:ai": "cd backend/fastapi-ai && python main.py",
    "dev:backend": "cd backend/nestjs-api && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:up": "docker-compose up -d",
    "health-check": "curl -s http://localhost:3000/api/health | jq",
    "lint": "cd frontend && npm run lint",
    "reset-db": "npm run setup-db",
    "seed-data": "chmod +x scripts/002_run_fake_data.sh && ./scripts/002_run_fake_data.sh",
    "setup": "cd frontend && npm install",
    "setup-db": "cd frontend && npm run setup-db",
    "setup:all": "npm run setup && npm run setup:backend && npm run setup:python",
    "setup:backend": "cd backend/nestjs-api && npm install",
    "setup:python": "bash scripts/setup-python.sh",
    "start": "cd frontend && npm start",
    "start-local": "chmod +x scripts/start-local.sh && ./scripts/start-local.sh",
    "test": "cd frontend && npm test",
    "test-db": "curl -s http://localhost:3000/api/test-db | jq",
    "verify-db": "cd frontend && npm run verify-db"
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "@apollo/client": "latest",
    "@neondatabase/serverless": "latest",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-aspect-ratio": "latest",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-context-menu": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-hover-card": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "latest",
    "@radix-ui/react-navigation-menu": "latest",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-radio-group": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-toggle": "latest",
    "@radix-ui/react-toggle-group": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@supabase/supabase-js": "latest",
    "@vercel/blob": "latest",
    "ai": "latest",
    "chart.js": "latest",
    "chartjs-adapter-date-fns": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "latest",
    "date-fns": "latest",
    "embla-carousel-react": "latest",
    "graphql": "latest",
    "graphql-ws": "latest",
    "i18next": "latest",
    "input-otp": "latest",
    "lucide-react": "^0.454.0",
    "maplibre-gl": "latest",
    "next": "14.2.16",
    "next-i18next": "latest",
    "next-themes": "latest",
    "react": "^18",
    "react-chartjs-2": "latest",
    "react-day-picker": "latest",
    "react-dom": "^18",
    "react-hook-form": "latest",
    "react-i18next": "latest",
    "react-resizable-panels": "latest",
    "recharts": "latest",
    "sonner": "latest",
    "subscriptions-transport-ws": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "postcss": "^8.5",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.11.0"
  }
}